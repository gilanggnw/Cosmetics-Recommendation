{
    "version": 2,
    "builds": [
      {
        "src": "flask_backend/api/app.py",
        "use": "@vercel/python"
      },
      {
        "src": "CosmeticsRecomendation/package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "flask_backend/api/app.py"
      },
      {
        "src": "/(.*)",
        "dest": "CosmeticsRecomendation/$1"
      }
    ]
  }
  